{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "description": "Next.js boilerplate with husky, lint-staged, eslint + prettier, jest, react-testing-library, storybook, ghaction and plop.",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "prettier:format": "prettier --write .",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,json}\"",
    "lint": "next lint",
    "eslint:format": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest test",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "postinstall": "husky install",
    "generate": "yarn plop --plopfile ./.plop/plopfile.js",
    "db:generate": "yarn prisma generate"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn prettier:format",
      "yarn eslint:format"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^2.9.11",
    "@prisma/client": "^4.10.1",
    "@reach/combobox": "^0.18.0",
    "@supabase/auth-helpers-nextjs": "^0.5.4",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/auth-ui-react": "^0.3.0",
    "@supabase/supabase-js": "^2.8.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/firebase": "^3.2.1",
    "@types/swiper": "^6.0.0",
    "apollo-server-micro": "^3.11.1",
    "axios": "^1.3.4",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.34.0",
    "cloudinary-react": "^1.8.1",
    "clsx": "1.2.1",
    "cookies": "^0.8.0",
    "firebase": "^9.17.1",
    "firebase-admin": "^11.5.0",
    "geolib": "^3.3.3",
    "graphql": "^16.6.0",
    "micro": "^10.0.1",
    "micro-cors": "^0.1.1",
    "next": "13.1.6",
    "nextjs-cors": "^2.1.2",
    "nexus": "^1.3.0",
    "phosphor-react": "^1.4.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-firebaseui": "^6.0.0",
    "react-hook-form": "^7.43.2",
    "react-icons": "^4.7.1",
    "react-swiper": "^0.1.5",
    "reflect-metadata": "^0.1.13",
    "swiper": "^9.1.0",
    "type-graphql": "^1.1.1",
    "use-google-maps-script": "^0.1.2",
    "use-places-autocomplete": "^4.0.0",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "6.5.16",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addon-storysource": "6.5.16",
    "@storybook/react": "6.5.16",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/cookie": "^0.5.1",
    "@types/micro-cors": "^0.1.3",
    "@types/node": "^18.14.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.52.0",
    "address": "^1.2.2",
    "autoprefixer": "10.4.13",
    "babel-jest": "29.4.3",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.10.2",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "jest-watch-typeahead": "2.2.2",
    "lint-staged": "13.1.2",
    "pinst": "3.0.0",
    "plop": "3.1.2",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.3",
    "prisma": "^4.10.1",
    "storybook-addon-next-router": "4.0.2",
    "storybook-dark-mode": "2.1.0",
    "tailwind-scrollbar": "2.1.0",
    "tailwindcss": "3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
